<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Test Wiki and IMDB Pusha functionality">
	<listeners>
		<listener class-name="com.UtilsLayer.ListenerClass"></listener>
	</listeners>
	<test name="Test Pusha the Rise">
		<groups>
			<run>
				<include name="google"></include>
				<include name="wiki"></include>
				<include name="imdb"></include>
				<include name="validation"></include>
			</run>
		</groups>
		<classes>
			<class name="com.TestLayer.GooglePageTest" />
			<class name="com.TestLayer.GoogleSearchPageTest" />
			<class name="com.TestLayer.WikipediaPageTest" />
			<class name="com.TestLayer.ImdbPageTest" />
			<class name="com.TestLayer.ValidationWIkiAndImdb"></class>
		</classes>
	</test> <!-- Test -->

</suite> <!-- Suite -->
